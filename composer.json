{
    "name": "buckleshusky/fontawesomeiconpicker",
    "type": "silverstripe-vendormodule",
    "description": "Adds a Font Awesome 5 icon picker for SilverStripe.",
    "keywords": [
        "silverstripe",
        "fontawesome",
        "font awesome",
        "icons",
        "picker"
    ],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Buckles Husky",
            "homepage": "http://doggershusky.com"
        }
    ],
    "require": {
        "silverstripe/framework": "~4.0",
        "silverstripe/vendor-plugin": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "buckleshusky\\FontAwesomeIconPicker\\": "src/"
        }
    },
    "config": {
        "process-timeout": 600
    },
    "extra": {
        "expose": [
            "css",
            "js",
            "javascript",
            "webfonts"
        ]
    }
}
